{
  "compilerOptions": {
		"baseUrl": "src",
    "target": "ES2015",
    "module": "ES2015",
    "declaration": true,
    "outDir": "lib",
    "allowSyntheticDefaultImports": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true
  }
}
